<VirtualHost *:80>
    ServerAdmin webmaster@localhost
    DocumentRoot ~code/Aurelie-Coudert/PHP-App1/public
    <Directory ~code/Aurelie-Coudert/PHP-App1/public>
        Options Indexes FollowSymLinks
        AllowOverride All
        Require all granted
        DirectoryIndex index.php
    </Directory>
    ErrorLog ${APACHE_LOG_DIR}/error.log
    CustomLog ${APACHE_LOG_DIR}/access.log combined
</VirtualHost>
